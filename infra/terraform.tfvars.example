# Example Terraform Variables Configuration
# Copy this file to terraform.tfvars and customize as needed

# Project configuration
project_name = "fedrag"
environment  = "dev"
aws_region   = "us-east-1"

# Common tags (optional - defaults are provided)
common_tags = {
  Project     = "FedRag"
  ManagedBy   = "Terraform"
  Application = "Privacy-First RAG Assistant"
  Owner       = "your-team-name"
  CostCenter  = "your-cost-center"
}

# Cognito Configuration (required for API authentication)
# These values will be available after deploying the Cognito infrastructure
cognito_user_pool_id        = "us-east-1_XXXXXXXXX"
cognito_user_pool_client_id = "your-cognito-client-id"

# Knowledge Base Configuration (required for API functionality)
# This value will be available after deploying the Knowledge Base infrastructure
knowledge_base_id = "your-knowledge-base-id"